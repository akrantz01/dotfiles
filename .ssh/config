Host github.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
Host gitlab.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
Host code.krantz.dev
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
Host home.krantz.dev
  IdentityFile ~/SyncThing/SSH-Keys/krantz.dev/pi

Host *.krantz.dev
  ProxyCommand bash -c '/usr/local/bin/cloudflared access ssh-gen --hostname $(echo %h | sed "s/dev/cloud/g"); ssh -tt %r@$(echo %h | sed "s/dev/cloud/g") >&2 <&1'

Host *.krantz.cloud
  ProxyCommand /usr/local/bin/cloudflared access ssh --hostname %h
  IdentityFile ~/.cloudflared/%h-cf_key
  CertificateFile ~/.cloudflared/%h-cf_key-cert.pub
